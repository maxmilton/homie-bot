    "sapper": "0.23.0",
    "stylelint": "9.6.0",
    "svelte": "2.13.5",
    "ts-jest": "^23.10.4",
    "tslib": "1.9.3",
    "typescript": "3.1.3"
  },
  "resolutions": {
    "**/@minna-ui/css": "link:../../minna-ui/css"
  }
